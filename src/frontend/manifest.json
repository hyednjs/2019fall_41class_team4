{
	"name":"PICEKT",
	"description":"SE project",
	"version":"1.0",
	"permissions":[
		"activeTab",
		"tabs"
	],
	"manifest_version":2,
	"background": {
        "scripts": [
            "anim.js"
        ],
        "persistent": true
    },
	"browser_action":{
		"default_icon": "img/icon.png",
		"default_title":"PICEKT",
		"default_popup":"request.html"
	},
	"content_scripts":[{
		"js":["jquery-3.4.1.min.js"],
		"matches":["<all_urls>"]
	}]
}
